{
  "name": "copilot-cli",
  "version": "0.1.0",
  "description": "CLI tool for querying Copilot.money data via CSV exports",
  "type": "module",
  "bin": {
    "copilot": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build && npm test -- --run",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "copilot",
    "copilot-money",
    "finance",
    "personal-finance",
    "cli",
    "budgeting",
    "transactions",
    "csv",
    "sqlite"
  ],
  "author": {
    "name": "Michael O'Boyle",
    "url": "https://oboyle.co"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaeloboyle/copilot-cli.git"
  },
  "bugs": {
    "url": "https://github.com/michaeloboyle/copilot-cli/issues"
  },
  "homepage": "https://github.com/michaeloboyle/copilot-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.13",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "csv-parse": "^5.5.0"
  }
}
